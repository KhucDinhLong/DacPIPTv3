<div class="modal-body">
    <div class="text-center">
        <h3 class="text-red">Danh sách seri không hợp lệ</h3>
    </div>
    <div class="text-center">
        @foreach (var item in LstInvalidSeri)
        {
            <h5 class="text-red">@item</h5>
        }
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-primary m-auto" @onclick="OnClose">OK</button>
</div>
